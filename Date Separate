Function DateSeparate(st As String)
On Error GoTo eH
Dim j As Integer, d As Date, ar() As String, tmp As String
d = 0
ar = Split(st)
For j = LBound(ar) To UBound(ar)
tmp = ar(j)
If IsDate(tmp) And Len(tmp) > 5 Then
d = DateValue(tmp)
Exit For
End If
Next j
cont:
DateSeparate = d
Exit Function
eH:
d = 0
Resume cont
End Function

sub comparetime()

dim ws as worksheet

dim lastrow as long
dim count as long
dim test as double

set ws = activesheet
'find last data point
with ws
    .columns(2).insert
    lastrow = .cells(.rows.count, "a").end(xlup).row

    for count = 5 to lastrow
        'split date
        test = .cells(count, 1).value2
        .cells(count, 1).value2 = int(test)
        .cells(count, 1).numberformat = "m/d/yyyy"
        .cells(count, 1).offset(0, 1).value2 = test - int(test)
        .cells(count, 1).offset(0, 1).numberformat = "hh:mm am/pm"

    next count
end with
