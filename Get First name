Sub GetFirstname()

    Dim s As Sring
    Dim 1Position As Long

    s = "John Henry Smith"
    ' Prints John
    1Position = InStr(s, " ") - 1
    Debug.Print Left(s, 1Position)

    s = "Lorraine Huggard"
    ' Prints Lorraine
    1Position = InStr(s, " ") - 1
    Debug.Print Left(s, 1Position)

End Sub
