Sub Extract_Data_from_PDF()

Dim myWorksheet As Worksheet
Dim NitroPDF_Path As String
Dim pathAndFileName As String
Dim shellPathName As String

Set MyWorksheet = ActiveWorkbook.Worksheets("D0")

NitroPDF_Path = "C:\Program Files\Nitro\Pro\13\NitroPDF.exe"
pathAndFileName = "C:\Users\Administrator\Downloads\1K23TNH_00033057"
shellPathName = NitroPDF_Path & " """ & pathAndFileName & """"

Call Shell( _
    pathName

Application.Wait Now + TimeValue("0:00:03")

SendKeys "%vpc"
SendKeys "^a"
SendKeys "^c"

MyWorksheet.Range("A1").PasteSpecial Paste:=xlPasteAll

Call Shell("TaskKill /F /IM Acrobat.exe", vbHide)

End Sub
